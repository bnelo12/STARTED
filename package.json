{
  "name": "started",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./static",
    "setup": "npm run clean && mkdir -p ./static",
    "start": "node ./bin/www",
    "build": "npm run setup && pug views/index.pug --out static/ && cp -r public/* static/ && cp -r html/* static/"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "morgan": "~1.7.0",
    "pug": "~2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "pug-cli": "^1.0.0-alpha6"
  }
}
